{
  "name": "__MSG_ext_name__",
  "description": "__MSG_ext_description__",
  "content_scripts": [
    {
      "js": [
        "content/index.tsx"
      ],
      "matches": [
        "https://www.fragrantica.pl/perfumy/*",
        "https://www.fragrantica.com/perfume/*",
        "https://www.parfumo.net/Perfumes/*"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "pages/popup/index.html",
    "default_icon": {
      "16": "images/icon.png",
      "32": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png",
      "256": "images/icon.png"
    }
  },
  "permissions": [
    "https://extension.isedo.pl/*"
  ],
  "web_accessible_resources": ["_locales/*.json"],
  "default_locale": "pl",
  "manifest_version": 2
}
